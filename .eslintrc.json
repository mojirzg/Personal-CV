{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next",
        "plugin:@next/next/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/no-cycle" : "error"
    }
}
